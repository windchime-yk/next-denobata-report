{
  "tasks": {
    "start": "deno run -A --watch=static/,routes/,components/ dev.ts",
    "build": "deno run -A dev.ts build",
    "preview": "deno run -A main.ts"
  },
  "lock": false,
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "npm:preact@10.15.1"
  },
  "imports": {
    "@/": "./",
    "$fresh/": "https://deno.land/x/fresh@1.4.3/",
    "preact": "npm:preact@10.15.1",
    "preact/hooks": "npm:preact@10.15.1/hooks",
    "preact/debug": "npm:preact@10.15.1/debug",
    "preact-render-to-string": "npm:preact-render-to-string@6.2.1",
    "@preact/signals": "npm:@preact/signals@1.1.3",
    "@preact/signals-core": "npm:@preact/signals-core@1.2.3",
    "twind": "npm:twind@0.16.19",
    "twind/sheets": "npm:twind@0.16.19/sheets",
    "std/": "https://deno.land/std@0.177.0/",
    "gfm": "https://deno.land/x/gfm@0.1.30/mod.ts",
    "marked": "npm:marked@10.0.0",
    "icons/": "https://deno.land/x/tabler_icons_tsx@0.0.2/tsx/",
    "deno-util/": "https://pax.deno.dev/windchime-yk/deno-util@v1.6.2/"
  },
  "lint": {
    "rules": {
      "tags": [
        "fresh",
        "recommended"
      ]
    },
    "exclude": [
      "_fresh"
    ]
  },
  "fmt": {
    "exclude": [
      "_fresh"
    ]
  }
}
